<?xml version="1.0" ?>
<sdf version="1.6">
  <model name="table_with_drawers">
    <!-- Table top -->
    <link name="table_top">
      <pose>0 0 0.5 0 0 0</pose>
      <visual name="table_visual">
        <geometry>
          <box>
            <size>0.6 0.5 0.05</size>
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="table_collision">
        <geometry>
          <box>
            <size>0.6 0.5 0.05</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Drawer 1 -->
    <link name="drawer_1">
      <pose>0 0.15 0.35 0 0 0</pose>  <!-- Moved forward (positive Y) -->
      <visual name="drawer_1_visual">
        <geometry>
          <box>
            <size>0.5 0.4 0.1</size>
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="drawer_1_collision">
        <geometry>
          <box>
            <size>0.5 0.4 0.1</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Drawer 2 -->
    <link name="drawer_2">
      <pose>0 0.15 0.2 0 0 0</pose>  <!-- Moved forward (positive Y) -->
      <visual name="drawer_2_visual">
        <geometry>
          <box>
            <size>0.5 0.4 0.1</size>
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="drawer_2_collision">
        <geometry>
          <box>
            <size>0.5 0.4 0.1</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Table back panel -->
    <link name="back_panel">
      <pose>0 -0.2 0.275 0 0 0</pose>
      <visual name="back_panel_visual">
        <geometry>
          <box>
            <size>0.5 0.05 0.45</size>
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="back_panel_collision">
        <geometry>
          <box>
            <size>0.5 0.05 0.45</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Table side panels -->
    <link name="left_panel">
      <pose>0.225 0 0.275 0 0 0</pose>
      <visual name="left_panel_visual">
        <geometry>
          <box>
            <size>0.05 0.4 0.45</size>
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="left_panel_collision">
        <geometry>
          <box>
            <size>0.05 0.4 0.45</size>
          </box>
        </geometry>
      </collision>
    </link>

    <link name="right_panel">
      <pose>-0.225 0 0.275 0 0 0</pose>
      <visual name="right_panel_visual">
        <geometry>
          <box>
            <size>0.05 0.4 0.45</size>
          </box>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="right_panel_collision">
        <geometry>
          <box>
            <size>0.05 0.4 0.45</size>
          </box>
        </geometry>
      </collision>
    </link>

    <!-- Table legs -->
    <link name="leg1">
      <pose>0.25 0.2 0.25 0 0 0</pose>
      <visual name="leg1_visual">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="leg1_collision">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <link name="leg2">
      <pose>-0.25 0.2 0.25 0 0 0</pose>
      <visual name="leg2_visual">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="leg2_collision">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <link name="leg3">
      <pose>0.25 -0.2 0.25 0 0 0</pose>
      <visual name="leg3_visual">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="leg3_collision">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <link name="leg4">
      <pose>-0.25 -0.2 0.25 0 0 0</pose>
      <visual name="leg4_visual">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
        <material>
          <ambient>0.4 0.2 0.1 1</ambient>
          <diffuse>0.7 0.4 0.2 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <collision name="leg4_collision">
        <geometry>
          <cylinder>
            <radius>0.02</radius>
            <length>0.5</length>
          </cylinder>
        </geometry>
      </collision>
    </link>

    <!-- 添加抽屉的滑动关节 -->
    <joint name="drawer_1_joint" type="prismatic">
      <parent>right_panel</parent>
      <child>drawer_1</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0.3</upper>
          <effort>50</effort>
          <velocity>0.5</velocity>
        </limit>
      </axis>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <joint name="drawer_2_joint" type="prismatic">
      <parent>right_panel</parent>
      <child>drawer_2</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>0</lower>
          <upper>0.3</upper>
          <effort>50</effort>
          <velocity>0.5</velocity>
        </limit>
      </axis>
      <pose>0 0 0 0 0 0</pose>
    </joint>

    <!-- 固定侧板到桌面 -->
    <joint name="left_panel_joint" type="fixed">
      <parent>table_top</parent>
      <child>left_panel</child>
    </joint>

    <joint name="right_panel_joint" type="fixed">
      <parent>table_top</parent>
      <child>right_panel</child>
    </joint>

    <!-- 固定后板到桌面 -->
    <joint name="back_panel_joint" type="fixed">
      <parent>table_top</parent>
      <child>back_panel</child>
    </joint>

    <!-- 固定桌腿到桌面 -->
    <joint name="leg1_joint" type="fixed">
      <parent>table_top</parent>
      <child>leg1</child>
    </joint>

    <joint name="leg2_joint" type="fixed">
      <parent>table_top</parent>
      <child>leg2</child>
    </joint>

    <joint name="leg3_joint" type="fixed">
      <parent>table_top</parent>
      <child>leg3</child>
    </joint>

    <joint name="leg4_joint" type="fixed">
      <parent>table_top</parent>
      <child>leg4</child>
    </joint>

  </model>
</sdf>
